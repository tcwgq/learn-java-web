 <body>
  <script type="text/javascript">
     var date = new Date();
	 document.write(date);
	 document.write("<br/>");
	 document.write(date.toLocaleString());
	 document.write("<br/>");
     document.write(date.getFullYear());
     document.write("<br/>");
	 document.write(date.getYear());
	 document.write("<br/>");
	 document.write(date.getMonth() + 1);
	 document.write("<br/>");
	 //getDay() 方法可返回表示星期的某一天的数字。
	 document.write(date.getDay());
  </script>
 </body>
