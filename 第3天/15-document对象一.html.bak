 <body>
	<input type="text" name="username" value="aaa" id="usernameid"/><br/>
 </body>
 <script type="text/javascript">
     //write() 方法可向文档写入 HTML 表达式或 JavaScript 代码。可列出多个参数(exp1,exp2,exp3,...) ，它们将按顺序被追加到文档中。
     var str = "abc";
	 document.write(str);
	 document.write("<br/>");
	 document.write("<hr/>");
	 //getElementById() 方法可返回对拥有指定 ID 的第一个对象的引用。
	 var username = document.getElementById("usernameid");
	 document.write(username);
	 document.write("<br/>");
	 document.write(username.value);
	 document.write("<br/>");
	 username.value="bbb";
	 document.write(username.value);
  </script>
