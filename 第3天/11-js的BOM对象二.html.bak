	 */
	 document.write("<br/>");
     var int=self.setInterval("clock()",50)
     function clock() {
     var t=new Date();
     document.getElementById("clock").value=t;
	 }
	 //setTimeout() 方法用于在指定的毫秒数后调用函数或计算表达式。
	 var ID = window.setTimeout("alert('123')", 3000);
	 document.write("<br/>");
	 //clearInterval() 方法可取消由 setInterval() 设置的 timeout。
